{
    "version": 2,
    "builds": [
      {
        "src": "quizo-backend/src/server.ts",
        "use": "@vercel/node"
      },
      {
        "src": "quizo-frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/quizo-backend/index.js" },
      { "src": "/(.*)", "dest": "/quizo-frontend/build/$1" }
    ]
  }
  